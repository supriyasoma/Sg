it('should filter dataset list based on search event', () => {
  // Initialize checkboxFilterDatasetList for testing
  component.checkboxFilterDatasetList = [
    { name: 'Dataset 1', subName: 'Subname 1' },
    { name: 'Dataset 2', subName: 'Subname 2' }
  ];

  // Mock event with a search term
  const searchEvent = { target: { value: 'Dataset 1' } };
  component.search(searchEvent);

  // Assert that datasetList and searchFilterDatasetList are updated correctly
  expect(component.searchFilterDatasetList.length).toBe(1);
  expect(component.searchFilterDatasetList[0].name).toBe('Dataset 1');
  expect(component.datasetList.length).toBe(1);
  expect(component.datasetList[0].name).toBe('Dataset 1');
});

it('should handle null or undefined search event', () => {
  // Initialize checkboxFilterDatasetList for testing
  component.checkboxFilterDatasetList = [
    { name: 'Dataset 1', subName: 'Subname 1' },
    { name: 'Dataset 2', subName: 'Subname 2' }
  ];

  // Simulate null or undefined event
  component.search(null);
  expect(component.datasetList).toEqual(component.referentialsValuesList);

  component.search(undefined);
  expect(component.datasetList).toEqual(component.referentialsValuesList);
});

it('should handle empty string search event', () => {
  // Initialize checkboxFilterDatasetList for testing
  component.checkboxFilterDatasetList = [
    { name: 'Dataset 1', subName: 'Subname 1' },
    { name: 'Dataset 2', subName: 'Subname 2' }
  ];

  // Simulate empty string event
  component.search('');
  expect(component.datasetList).toEqual(component.referentialsValuesList);
});
