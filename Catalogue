  it('should handle version select when datasetContent and versionData are defined', (done) => {
    component.datasetContent = [
      { version: '1.0', data: 'data1' },
      { version: '2.0', data: 'data2' }
    ];
    component.isLoadFile = false;
    component.rowData = 'rowData';

    component.onVersionSelect('2.0');

    setTimeout(() => {
      // Assertions to check setTimeout functionality
      expect(mockGridApi.setRowData).toHaveBeenCalledWith('data2');
      expect(component.isLoadFile).toBe(false);

      done();
    }, 500);

    // Add assertions to cover other lines
    expect(component.showSpinner).toBe(false);
    expect(component.dataIsValid.emit).toHaveBeenCalledWith(false);
    expect(component.isPushed.emit).toHaveBeenCalledWith(false);
    expect(component.editChangeOnVersion.emit).toHaveBeenCalledWith(false);
    expect(component.alertValue).toEqual({ class: "", message: "" });
    expect(component.showSpinner).toBe(false);
  });
