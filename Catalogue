getCSVFileDefinitionFromGridData = jasmine.createSpy('getCSVFileDefinitionFromGridData').and.callFake(
    (fileName: string, gridApi: any): CSVFileDefinition => {
      const mockCSVFileDefinition = new CSVFileDefinition(fileName, ['mockContentLine1', 'mockContentLine2']);
      return mockCSVFileDefinition;
    }
  );
